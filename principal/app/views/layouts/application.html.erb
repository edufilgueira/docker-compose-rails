<!DOCTYPE html>
<html>
<head>
  <title><%= nome_sistema %></title>

  <!-- Main css styles -->
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>

  <!-- Main javascript files -->
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>

<body class="pace-done">

  <!-- Wrapper-->
  <div id="wrapper">

    <!-- Navigation -->
    <%= render 'layouts/'+@navigation_partial %>

    <!-- Page wraper -->
    <div id="page-wrapper" class="gray-bg">

      <!-- Page wrapper -->
      <%= render 'layouts/topnavbar' %>
      <%= render partial: 'layouts/breadcrumb' %>
      <%= render partial: 'layouts/flash_messages' %>
      <!-- Main view  -->
      <div class="wrapper wrapper-content animated fadeInRight">
        <div class="ibox" id="box-loading">
          <div class="ibox-content box-loading-content">
            <div class="sk-spinner sk-spinner-three-bounce">
              <div class="sk-bounce1"></div>
              <div class="sk-bounce2"></div>
              <div class="sk-bounce3"></div>
            </div>

            <%= yield %>

          </div>
        </div>
      </div>

      <!-- Footer -->
      <%= render 'layouts/footer' %>
      <%= render 'layouts/modal' %>

    </div>
    <!-- End page wrapper-->

  </div>
  <!-- End wrapper-->

  <%= yield :javascript %>
</body>
</html>
<script type="text/javascript"> root_path = '<%= root_path %>'; </script>
