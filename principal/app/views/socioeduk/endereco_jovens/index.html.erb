<%- model_class = Socioeduk::EnderecoJovem -%>
<% content_for :title do %>
<%=  I18n.t('activerecord.models.socioeduk_endereco_jovem', default: Socioeduk::EnderecoJovem.model_name.human.pluralize.titleize) %>
<% end -%>

<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-title">
        <h5><%=  I18n.t('activerecord.models.socioeduk_endereco_jovem', default: Socioeduk::EnderecoJovem.model_name.human.pluralize.titleize) %></h5>
        <div class="ibox-tools">
          <a class="collapse-link">
            <i class="fa fa-chevron-up"></i>
          </a>
          <a class="close-link">
            <i class="fa fa-times"></i>
          </a>
        </div>
      </div>
      <div class="ibox-content">
        <div class="row">
          <div class="col-sm-12 b-r">
            <%= link_to_new new_socioeduk_endereco_jovem_path %>
            <table class="table table-hover table-sm">
              <thead>
                <tr>
                                    <th><%= model_class.human_attribute_name(:jovem) %></th>
                                    <th><%= model_class.human_attribute_name(:cidade) %></th>
                                    <th><%= model_class.human_attribute_name(:rua) %></th>
                                    <th><%= model_class.human_attribute_name(:numero) %></th>
                                    <th><%= model_class.human_attribute_name(:complemento) %></th>
                                    <th><%= model_class.human_attribute_name(:bairro) %></th>
                                    <th><%= model_class.human_attribute_name(:cep) %></th>
                                    <th width="18%"><%= I18n.t('helpers.actions') %></th>
                </tr>
              </thead>
              <tbody>
                <% @socioeduk_endereco_jovens.each do |socioeduk_endereco_jovem| %>
                <tr>
                                    <td><%= socioeduk_endereco_jovem.jovem.nome %></td>
                                    <td><%= socioeduk_endereco_jovem.cidade.nome %></td>
                                    <td><%= socioeduk_endereco_jovem.rua %></td>
                                    <td><%= socioeduk_endereco_jovem.numero %></td>
                                    <td><%= socioeduk_endereco_jovem.complemento %></td>
                                    <td><%= socioeduk_endereco_jovem.bairro %></td>
                                    <td><%= socioeduk_endereco_jovem.cep %></td>
                                    <td>
                    <%= link_to_show(socioeduk_endereco_jovem) %>
                    <%= link_to_edit(edit_socioeduk_endereco_jovem_path(socioeduk_endereco_jovem)) %>
                    <%= link_to_delete(socioeduk_endereco_jovem) %>
                  </td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
